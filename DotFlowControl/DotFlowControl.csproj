<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageId>DotControlFlow</PackageId>
        <PackageVersion>0.0.1</PackageVersion>
        <Title>DotControlFlow</Title>
        <Authors>Patrick Phillips</Authors>
        <Description>A basic type helper to make "failure-risk" functions easier to read and comprehend</Description>
        <Copyright>MIT</Copyright>
        <PackageLicenseUrl>https://github.com/RNGKing/DotControlFlow/blob/main/LICENSE</PackageLicenseUrl>
        <RepositoryUrl>https://github.com/RNGKing/DotControlFlow</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <PackageTags>Class Library, Result</PackageTags>
        <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
        <AssemblyVersion>0.0.1</AssemblyVersion>
    </PropertyGroup>

</Project>
